<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.bluesky.automationjiahua.ui.interlock.InterlockDetailFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/rectangle">

            <TextView
                android:id="@+id/tv_detail_interlock_column_domain"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="所属区域"
                android:theme="@style/text_column_style"

                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_domain"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="domain" />

            <TextView
                android:id="@+id/tv_detail_interlock_column_tag"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="设备位号"
                android:theme="@style/text_column_style"

                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_tag"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_detail_interlock_content_tag"
                tools:text="tag" />

            <TextView
                android:id="@+id/tv_detail_interlock_column_device_name"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="用途"
                android:theme="@style/text_column_style"
                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_device_name"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_detail_interlock_content_device_name"
                tools:text="affect" />

            <TextView
                android:id="@+id/tv_detail_interlock_column_control_value"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="参数"
                android:theme="@style/text_column_style"
                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_control_value"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_detail_interlock_content_control_value"
                tools:text="parameter" />

            <TextView
                android:id="@+id/tv_detail_interlock_column_interlock_device_name"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="仪表名称"
                android:theme="@style/text_column_style"
                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_interlock_device_name"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_detail_interlock_content_interlock_device_name"
                tools:text="name" />

            <TextView
                android:id="@+id/tv_detail_interlock_column_interlock_device_tag"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="测量范围"
                android:theme="@style/text_column_style"
                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_interlock_device_tag"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_detail_interlock_content_interlock_device_tag"
                tools:text="range" />

            <TextView
                android:id="@+id/tv_detail_interlock_column_action_type"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="信号及规格"
                android:theme="@style/text_column_style"
                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_action_type"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_detail_interlock_content_action_type"
                tools:text="standard" />

            <TextView
                android:id="@+id/tv_detail_interlock_column_remark"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="连接方式及标准"
                android:theme="@style/text_column_style"
                app:layout_constraintBottom_toBottomOf="@id/tv_detail_interlock_content_remark"
                app:layout_constraintEnd_toStartOf="@id/guideline6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_detail_interlock_content_remark"
                tools:text="mode" />





            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/line"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.25" />


            <!--    <TextView
                    android:id="@+id/tv_detail_content_serial"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"

                    app:layout_constraintStart_toStartOf="@id/guideline6"
                    app:layout_constraintTop_toBottomOf="@id/tv_detail_content_number"
                    tools:text="serial" />-->

            <TextView
                android:id="@+id/tv_detail_interlock_content_domain"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="domain" />

            <TextView
                android:id="@+id/tv_detail_interlock_content_tag"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toBottomOf="@id/tv_detail_interlock_content_domain"
                tools:text="tag" />

            <TextView
                android:id="@+id/tv_detail_interlock_content_device_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toBottomOf="@id/tv_detail_interlock_content_tag"
                tools:text="affect" />

            <TextView
                android:id="@+id/tv_detail_interlock_content_control_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toBottomOf="@id/tv_detail_interlock_content_device_name"
                tools:text="parameter" />

            <TextView
                android:id="@+id/tv_detail_interlock_content_interlock_device_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintBottom_toTopOf="@id/tv_detail_interlock_content_interlock_device_tag"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toBottomOf="@id/tv_detail_interlock_content_control_value"
                tools:text="A low-cost Android screen adaptation solution (今日头条屏幕适配方案终极版，一个极低成本的 Android 屏幕适配方案)" />

            <TextView
                android:id="@+id/tv_detail_interlock_content_interlock_device_tag"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toBottomOf="@id/tv_detail_interlock_content_interlock_device_name"
                tools:text="range" />

            <TextView
                android:id="@+id/tv_detail_interlock_content_action_type"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toBottomOf="@id/tv_detail_interlock_content_interlock_device_tag"
                tools:text="standard" />

            <TextView
                android:id="@+id/tv_detail_interlock_content_remark"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="8dp"
                android:textSize="24sp"
                android:theme="@style/text_column_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline6"
                app:layout_constraintTop_toBottomOf="@id/tv_detail_interlock_content_action_type"
                tools:text="mode" />





        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</LinearLayout>